<body>
    <div class="container-fluid" id="navBarContainer">
        <nav>
            <div class="row" id="navBarRow">
                <div id="logoID">
                    <a href="/"><span class="text-center" id="logo">Make a Quiz!</span></a>
                </div>
                <div id="navBarLinks">
                    <ul id="ulID">
                        <li>
                            {{#if username}}
                            <p id="username">Hi {{username}}</p>
                            {{/if}}
                            <a href='/profile_page'><i class="fas fa-user"></i></a>&nbsp;
                            <a href=""><i class="fas fa-question"></i></a>
                            {{#if username}}
                            <p id="logout">logout</p>
                            {{/if}}
                        </li>
                    </ul>
                </div>
            </div>
        </nav>
    </div>
    
    {{{ body }}}
    
